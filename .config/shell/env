#!/usr/bin/env sh

# Set $ENV so that the startup scripts will run correctly if sh is started as a
# non-login interactive shell under a different login shell
export ENV=~/.config/sh/interactive

# Set BASH_ENV, which is run for non-interactive shells
export BASH_ENV=~/.config/bash/env

. ~/.config/shell/env_functions

#source ~/.config/shell/env_local
